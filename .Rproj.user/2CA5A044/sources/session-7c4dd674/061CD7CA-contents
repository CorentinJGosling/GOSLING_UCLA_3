  dat[, c("Q3_DIM_AMI", 
          "Q3_DIM_AUTO",
          "Q3_DIM_SORTIE",
          "Q3_DIM_LOISIR",
          "Q3_DIM_EDU",
          "Q3_DIM_RELIG")],
  lower = list(continuous = my_fn), 
  progress = FALSE
  ) 

dat$Q3_DIM_TOT = apply(dat[, c("Q3_DIM_AMI", 
          "Q3_DIM_AUTO",
          "Q3_DIM_SORTIE",
          "Q3_DIM_LOISIR",
          "Q3_DIM_EDU",
          "Q3_DIM_RELIG")], 1, mean)
```


\newpage{}

#	Explorer les liens entre ces sous-dimensions et : la PI 1 :Q5.1 ?

```{r, fig.height=6, echo=FALSE, warning=FALSE, message=FALSE}
ggscatterstats(dat,
               Q3_DIM_TOT, 
               Q5_1_parti_insti,
           bf.message = FALSE,
           point.height.jitter = 0.3,
