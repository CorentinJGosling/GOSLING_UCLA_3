```{r, echo=FALSE}
dat[, c("Q3_DIM_AMI", 
          "Q3_DIM_AUTO",
          "Q3_DIM_SORTIE",
          "Q3_DIM_LOISIR",
          "Q3_DIM_EDU",
          "Q3_DIM_RELIG")] %>%
  psy::cronbach()

