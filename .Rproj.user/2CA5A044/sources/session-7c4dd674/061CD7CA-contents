dat[, c("Q3_1_AVIS_VQ_ENF", "Q3_1_AVIS_VQ_PARENT", "Q3_1_AVIS_VQ_AF", "Q3_1_AVIS_VQ_REFASE", "Q3_1_AVIS_VQ_CONSFAM", "Q3_1_AVIS_VQ_JUGE", "Q3_1_AVIS_VQ_CAD_ASE")] %>%
