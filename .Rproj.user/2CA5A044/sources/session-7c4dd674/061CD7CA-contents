```{r, echo=FALSE, warning=FALSE, results='hide', message=FALSE}
library(tidyverse)
library(ggstatsplot)
library(car)
library(emmeans)
library(GGally)
```


```{r echo=FALSE}
dat = readxl::read_excel("data_analyses1.xlsx") %>%
  filter(!is.na(Q1_AF4_NB_ENF) & !is.na(Q1_AF2_SEX) & !is.na(Q1_AF1_ANCIEN))

cols.num <- c('Q1_AF4_NB_ENF', 'Q1_AF2_SEX', 'Q1_AF1_ANCIEN', 'Q2_6_FREQ_RECUEIL_AVIS',
              'Q3_2_DAV_AVIS',
             'Q2_PERCEP_ENF_PRO', 'Q2_PERCEP_ENF_GEN', 'Q2_3_INFO_MESURE',
             "Q5_1_parti_insti", 
             "Q5_3_parti_insti",
             "Q5_4_MOD_SOUHAIT_PART",
"Q5_6_EQUIP_PPE", "Q5_9_ECOUTE_ENFANT",
"Q5_2_AF", "Q5_2_PAR",    
"Q5_2_JUG", "Q5_2_RTASE",  
"Q5_2_REN", "Q5_2_RSE", "Q5_2_PSE_ASE"
             )

dat[, cols.num] <- sapply(dat[, cols.num], as.numeric)

dat[,which(grepl("Q5_3", colnames(dat), fixed = TRUE))] <- sapply(dat[,which(grepl("Q5_3", colnames(dat), fixed = TRUE))], as.numeric)
dat[,which(grepl("Q3_1_DIM", colnames(dat), fixed = TRUE))] <- sapply(dat[,which(grepl("Q3_1_DIM", colnames(dat), fixed = TRUE))], as.numeric)

dat[,which(grepl("Q2_2", colnames(dat), fixed = TRUE))] <- sapply(dat[,which(grepl("Q2_2", colnames(dat), fixed = TRUE))], as.numeric)
dat[,which(grepl("Q2_8_", colnames(dat), fixed = TRUE))] <- sapply(dat[,which(grepl("Q2_8_", colnames(dat), fixed = TRUE))], as.numeric)

dat$Q1_AF2_SEX[dat$Q1_AF2_SEX==0] <- "Homme"
dat$Q1_AF2_SEX[dat$Q1_AF2_SEX==1] <- "Femme"
dat$Q1_AF1_ANCIEN[dat$Q1_AF1_ANCIEN==1] <- "<3ans"
dat$Q1_AF1_ANCIEN[dat$Q1_AF1_ANCIEN==2] <- "3-8ans"
dat$Q1_AF1_ANCIEN[dat$Q1_AF1_ANCIEN==3] <- ">8ans"
dat$Q1_AF1_ANCIEN <- factor(dat$Q1_AF1_ANCIEN, 
                            levels=c('<3ans', "3-8ans",">8ans"))
dat$Q5_7_LEG_AF[dat$Q5_7_LEG_AF==0] <- "Pas légitime"
dat$Q5_7_LEG_AF[dat$Q5_7_LEG_AF==2] <- "Légitime"
dat$Q5_7_LEG_AF <- factor(dat$Q5_7_LEG_AF, 
                            levels=c('Pas légitime', "Légitime"))

```

