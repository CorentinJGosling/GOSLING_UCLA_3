# Analyses complémentaires ----------------

# le score AF se dégage du score des autres pros
```{r, echo=FALSE}
dat_bar = dat %>%
  mutate(ID=1:nrow(dat)) %>%
  select(ID, starts_with("Q3_1_AVIS")) %>%
  pivot_longer(cols=starts_with("Q3_1_AVIS"), names_to="Q3", values_to = "value") 

dat_bar_agg =  dat_bar %>%
  group_by(Q3) %>%
  summarise(sd = sd(value),
            value = mean(value)
            ) %>%
  mutate(ylo = ifelse(value-sd < 0, 0, value-sd), 
         yup = value+sd)
dat_bar$Q3 = gsub("Q3_1_AVIS_", "", dat_bar$Q3)
p1 = glm(value ~ Q3,
          data=dat_bar,
          family="poisson")

Anova(p1, type="II",test="LR")

marg = emmeans(p1,~ Q3)
pairs(marg, adjust="tukey")

ggplot(dat_bar_agg, aes(x = Q3, y = value, fill = Q3)) + 
  geom_bar(stat = "identity", alpha = 0.5) +
  coord_flip() +
  labs(y="Nombre moyen de citations par participant") +
  theme_bw() +
  theme(legend.position="none")
  
```

\newpage{}

# le score AF à cette question (Q3.1 AF) est lié à la question Q5.2 
